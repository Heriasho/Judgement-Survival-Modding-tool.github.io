# Ability Effects

statuseffects.base.<baseName>.sprite=[string]

statuseffects.entity.<entityName>.parents=[list of entity parents] /*statuseffects.base.negative*/
statuseffects.entity.<entityName>.effect.accuracy=[double]
statuseffects.entity.<entityName>.effect.evade=[double]
statuseffects.entity.<entityName>.effect.attackspeed=[double]
statuseffects.entity.<entityName>.effect.damage=[double]
statuseffects.entity.<entityName>.effect.defense=[double]
statuseffects.entity.<entityName>.effect.walkspeed=[double]
statuseffects.entity.<entityName>.name=[string]

